<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>게시판 작성</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Abel&family=Barlow:wght@200;300;400;500&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <%- include ("common/header.ejs")%>
    <div id="container">
        <div class="cont1">
            <%- include ("common/banner.ejs")%>
            <div class="textWrap">
                <p class="m_text">FROPERTIES GRID FULLWIDTH</p>
                <p class="s_text">Index/FROPERTIES GRID FULLWIDTH</p>
            </div>
        </div>
        <div class="brdlist_cont2">
            <div class="center">
                <div class="searchWrap">
                    <form action="/search" method="get">
                            <div class="searchBox">
                                <select name="searchCategory">
                                    <option value="brdtitle">제목</option>
                                    <option value="brdcontext">작성자</option>
                                </select>
                                <input class="searbox" type="text" name="searchResult">
                                <button class="searbtn" type="submit">검색</button>
                                <p class="total">Total : </p>
                            </div>
                    </form>
                </div>
                <div class="boxes">
                    <div class="box">
                        <div class="imgs">img</div>
                        <div class="no">NO.</div>
                        <div class="subject">subject</div>
                        <div class="name">name</div>
                        <div class="email">email</div>
                        <div class="time">time</div>
                    </div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                    <div class="box"></div>
                </div>
                <a class="contactbtn" href="/brdinsert">Contact</a>
            </div>
        </div>
    </div>
    <%- include ("common/footer.ejs")%>
</body>
</html>